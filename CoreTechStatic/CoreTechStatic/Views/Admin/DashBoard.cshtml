<div class="row mt-3">
    <div class="col-md-4 mt-3">
        <div class="card  bg-primary  text-white">
            <div class="card-header">
                Total Product
            </div>
            <div class="card-body d-flex justify-content-between">
                <div>
                    Count
                </div>
                <div>
                    @ViewBag.TotalProduct
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4 mt-3">
        <div class="card bg-primary text-white">
            <div class="card-header">
                Total Category
            </div>
            <div class="card-body d-flex justify-content-between">
                <div>
                    Count
                </div>
                <div>
                    @ViewBag.TotalCatgory
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4 mt-3 ">
        <div class="card bg-secondary text-white">
            <div class="card-header">
                Active
            </div>
            <div class="card-body d-flex justify-content-between">
                <div>
                    Count
                </div>
                <div>
                    @ViewBag.Active
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4 mt-3">
        <div class="card bg-danger text-white">
            <div class="card-header">
                Total Product
            </div>
            <div class="card-body d-flex justify-content-between">
                <div>
                    Out Of Stock
                </div>
                <div>
                    @ViewBag.OutOfStock
                </div>
            </div>
        </div>
    </div>

</div>