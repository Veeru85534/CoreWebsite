@model IEnumerable<CoreTechStatic.Models.User>


<table class="table table-bordered table-sm">
    <thead>
        <tr>
            <th>User ID</th>
            <th>Full Name</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
            <th>Email Id</th>
            <th>Created Date</th>
            <th>Address</th>
            <th>Shipping Address</th>
            <th>City</th>
            <th>State</th>
            <th>Country</th>
            <th>Pin Code</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var iteam in Model)
        {
            <tr>
                <td>@iteam.User_Id</td>
                <td>@iteam.User_Name</td>
                <td>@iteam.First_Name</td>
                <td>@iteam.Last_Name</td>
                <td>@iteam.Phone_No</td>
                <td>@iteam.Email_Id</td>
                <td>@iteam.Created_Date</td>
                <td>@iteam.Address</td>
                <td>@iteam.Address</td>
                <td>@iteam.ShippingAddress</td>
                <td>@iteam.City</td>
                <td>@iteam.State</td>
                <td>@iteam.Pin_Code</td>          
            </tr>
        }

    </tbody>
</table>